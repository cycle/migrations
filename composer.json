{
    "name": "cycle/migrations",
    "type": "library",
    "description": "Database migrations, migration scaffolding",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Titov / Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "replace": {
        "spiral/migrations": "*"
    },
    "require": {
        "php": ">=7.2",
        "cycle/database": "^1.0",
        "spiral/core": "^2.8",
        "spiral/files": "^2.8",
        "spiral/tokenizer": "^2.8",
        "spiral/reactor": "^2.8"
    },
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Cycle\\Migrations\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.9",
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.3",
        "spiral/code-style": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Cycle\\Migrations\\Tests\\": "tests/Migrations",
            "Cycle\\Migrations\\Fixtures\\": "tests/Fixtures"
        }
    },
    "scripts": {
        "test": [
            "phpcs --standard=phpcs.xml",
            "psalm --no-cache",
            "phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
